#include <stdio.h>
#include <stdlib.h>

int main ()
{
	
	unsigned char		*displaydata;
	

	
	/* allocate dynamic memory for display[2] */
	displaydata = (unsigned char *)calloc(512*512*2,1);
	if (displaydata == NULL);
	{
		printf("Unable to allocate 512 x 512 memory\n");
		exit(0);
	}
	
	return(0);
}
